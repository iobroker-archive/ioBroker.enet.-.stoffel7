    "version": "0.2.0",
    "configurations": [  
        {
    
            "name": "Launch normal",
            "program": "${workspaceFolder}/src/main.ts",
            "args": [
                "--instance",
                "0",
                "--force",
                "--logs",
                "--debug"
            ],
            "request": "launch",
            "stopOnEntry": true,
            "console": "internalConsole",
            "outputCapture": "std",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
}